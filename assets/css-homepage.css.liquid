.header {
	position: fixed;
	top: 0;
	left: 0;
	z-index: 150;
	width: 100%;
	height: 85px;
	display: flex;
	align-items: center;
	justify-content: space-between;
	background-color: #fff;
	transition: background-color .4s ease-in-out
}

@media only screen and (min-width:1191px) {
	.header .menu-icon {
		height: 100%
	}
}

@media only screen and (max-width:1190px) {
	.header {
		height: 60px
	}
	.header .icon-logo-crocodile {
		transform: scale(1.3)
	}
	.l-menu-left {
		margin-right: 45px
	}
}

@media only screen and (min-width:1191px) {
	.main-nav {
		flex-grow: 1;
		height: 100%
	}
	.main-nav .main-nav-tabs {
		display: flex;
		align-items: center;
		justify-content: space-between;
		height: 100%
	}
	.main-nav .nav-lvl0-wrapper {
		display: flex;
		align-items: center;
		height: 100%;
		width: 100%
	}
	.main-nav .nav-lvl0 {
		margin-left: auto;
		margin-right: 20px
	}
	.main-nav .nav-footer,
	.main-nav .nav-lvl0,
	.main-nav .nav-lvl0-link,
	.main-nav .nav-lvl0-tab {
		display: flex;
		align-items: center;
		height: 100%
	}
	.main-nav .nav-lvl0-link {
		padding: 0 18px
	}
	.main-nav .nav-lvl0-label {
		padding-top: 5px;
		border-bottom: 2px solid transparent;
		transition: border .4s ease-out
	}
	.main-nav .nav-lvl1 {
		display: flex;
		flex-flow: row wrap
	}
	.main-nav .rich-menu,
	.main-nav .sub-menu {
		display: none;
		position: fixed;
		top: 85px;
		opacity: 0;
		overflow: hidden;
		transition: opacity .4s ease-out
	}
	.main-nav .nav-bg {
		position: fixed;
		top: 85px;
		bottom: 0;
		left: 0;
		z-index: 0;
		height: 0;
		width: 100%;
		background-image: linear-gradient(to bottom, #fff 400px, rgba(41, 41, 41, .2) 400px);
		pointer-events: none
	}
	.main-nav .rich-menu {
		left: 0;
		width: 100%;
		z-index: 1
	}
	.main-nav .sub-menu {
		right: 0;
		height: 0;
		min-height: 0;
		background-color: #fff;
		color: #292929;
		transition: opacity .4s ease-out, min-height .4s ease-out .4s;
		z-index: 2
	}
	.main-nav .sub-menu .nav-lvl2-link {
		display: flex;
		align-items: center;
		justify-content: space-between;
		text-transform: capitalize
	}
	.main-nav .nav-lvl0-wrapper:hover .sub-menu {
		display: block
	}
	.main-nav .nav-lvl0-wrapper:focus-within .rich-menu,
	.main-nav .nav-lvl0-wrapper:focus-within .sub-menu {
		display: block
	}
	.main-nav .nav-lvl0-tab:focus-within .nav-lvl0-label,
	.main-nav .nav-lvl0-tab:hover .nav-lvl0-label {
		border-color: #e5e5e5
	}
	.main-nav .nav-lvl0-tab:focus-within .sub-menu,
	.main-nav .nav-lvl0-tab:hover .sub-menu {
		height: auto;
		min-height: 325px;
		opacity: 1
	}
	.main-nav .nav-lvl0-tab:focus-within .nav-bg {
		height: calc(100vh - 85px)
	}
	.main-nav .nav-lvl0-tab:focus-within .rich-menu {
		height: auto;
		min-height: 400px;
		opacity: 1
	}
	.main-nav .nav-lvl1,
	.main-nav .nav-wrapper {
		height: 100%
	}
	.main-nav .l-padding-menu {
		padding-top: 40px;
		padding-bottom: 60px
	}
	.main-nav .nav-lvl1-link {
		display: inline-block;
		margin-bottom: 10px
	}
	.main-nav .nav-lvl1-label {
		color: #545454
	}
	.main-nav .nav-lvl2-link {
		display: inline-block;
		margin-bottom: 7px;
		transition: color .2s linear
	}
	.main-nav .nav-lvl2-link:hover {
		color: #999
	}
	.main-nav .nav-thematic .btn-cta {
		height: 24px;
		padding: 0 12px
	}
	.main-nav .nav-footer-link {
		margin-right: 20px
	}
	.non-critical-loading .main-nav .rich-menu {
		height: 0;
		min-height: 0;
		background-color: #fff;
		transition: opacity .4s ease-out, min-height .4s ease-out .4s
	}
	.non-critical-loading .main-nav .nav-lvl0-wrapper:hover .rich-menu {
		display: block
	}
	.non-critical-loading .main-nav .nav-lvl0-tab:hover .nav-bg {
		height: calc(100vh - 85px)
	}
	.non-critical-loading .main-nav .nav-lvl0-tab:hover .rich-menu {
		height: auto;
		min-height: 400px;
		opacity: 1
	}
	.nav-bg-layer {
		position: fixed;
		top: 0;
		left: 0;
		z-index: 149;
		width: 100%;
		height: 485px;
		transform: translate3d(0, -100%, 0);
		background-color: #fff;
		transition: transform .2s cubic-bezier(.14, .47, .96, .63)
	}
	.nav-bg-layer.is-opened {
		transform: translate3d(0, 0, 0)
	}
}

.header .nav-lvl0-label-custom {
	color: transparent;
	text-shadow: 0 0 0 #292929
}

.header .menu-icon {
	display: flex;
	align-items: center;
	font-size: 2.5rem
}

.header-transparent-enabled .header.is-transparent {
	background-color: transparent;
	color: #fff
}

.header-transparent-enabled .header.is-transparent .logo-lacoste {
	filter: brightness(0) invert(1)
}

.header-transparent-enabled .header.is-transparent .icon-logo-crocodile {
	color: #fff;
	fill: transparent
}

.header-transparent-enabled .header.is-transparent .nav-lvl0-label-custom {
	color: #fff
}

.main-nav .icon-indicator {
	font-size: 3rem;
	fill: #999
}

.minicart-count {
	position: absolute;
	top: 0;
	right: 0;
	width: 13px;
	height: 13px;
	line-height: 13px;
	border-radius: 100%;
	background: #105a33;
	color: #fff;
	font-size: .9rem;
	text-align: center
}

@media only screen and (max-width:1190px) {
	.minicart-wrapper {
		margin-right: 15px
	}
	.sub-nav {
		position: fixed;
		top: 0;
		left: 0;
		z-index: 200;
		width: 100%;
		height: 100vh;
		background-color: #fff;
		transform: translate3d(100%, 0, 0);
		transition: transform .4s ease-out
	}
	.sub-nav.is-opened {
		transform: translate3d(0, 0, 0)
	}
	.main-nav {
		position: absolute
	}
	.main-nav .btn-burger {
		position: fixed;
		top: 30px;
		transform: translateY(-50%);
		margin-top: 4px;
		right: calc(1 * (100vw - 17px)/ 25);
		right: calc(1 * (100vw)/ 25);
		font-size: 3rem
	}
	.main-nav .nav-header {
		display: flex;
		align-items: center;
		justify-content: space-between;
		height: 60px;
		border-bottom: 1px solid #f4f4f4
	}
	.main-nav .dir-rtl .nav-header {
		flex-direction: row-reverse
	}
	.main-nav .nav-back {
		font-size: 2.3rem
	}
	.main-nav .dir-rtl .nav-back {
		margin-left: 0;
		margin-right: 20px
	}
	.main-nav .nav-close {
		font-size: 4rem;
		transform: translateX(5px)
	}
	.main-nav .nav-close .icon-svg {
		display: block
	}
	.main-nav .nav-wrapper {
		height: calc(100vh - 60px)
	}
	.main-nav .nav-wrapper.is-active {
		overflow-y: auto;
		-webkit-overflow-scrolling: touch;
		-ms-overflow-style: -ms-autohiding-scrollbar
	}
	.main-nav .nav-lvl0,
	.main-nav .nav-lvl1,
	.main-nav .nav-lvl2 {
		display: flex;
		flex-direction: column;
		margin-top: 16px
	}
	.main-nav .nav-lvl0-link,
	.main-nav .nav-lvl1-link,
	.main-nav .nav-lvl2-link {
		display: block
	}
	.main-nav .nav-lvl0-label,
	.main-nav .nav-lvl1-label,
	.main-nav .nav-lvl2-link {
		display: flex;
		align-items: center;
		justify-content: space-between;
		margin-bottom: 20px;
		letter-spacing: 0;
		text-transform: capitalize;
		font-weight: 400
	}
	.main-nav .nav-thematic {
		background-color: #f4f4f4
	}
	.main-nav .nav-thematic-header {
		position: absolute;
		top: 30px;
		pointer-events: none
	}
	.main-nav .nav-thematic-tags {
		padding-top: 60px
	}
	.main-nav .nav-footer {
		margin-top: auto;
		padding-bottom: 60px
	}
	.main-nav .nav-footer-link {
		display: flex;
		align-items: center;
		width: 100%;
		height: 80px;
		border-top: 1px solid #f4f4f4;
		border-bottom: 1px solid #f4f4f4
	}
	.main-nav .l-icon-margin {
		margin-right: 10px
	}
	.main-nav .dir-rtl .l-icon-margin {
		margin-left: 10px;
		margin-right: 0
	}
	.main-nav .menu-icon.menu-icon {
		flex-direction: column-reverse;
		align-items: flex-start;
		font-size: 3rem
	}
}

@media only screen and (max-width:767px) {
	.main-nav .btn-burger {
		right: calc(1 * (100vw - 17px)/ 25);
		right: calc(1 * (100vw)/ 25)
	}
	.main-nav .nav-wrapper {
		height: calc(var(--vh-mob, 1vh) * 100 - 60px)
	}
	.main-nav .nav-lvl1,
	.main-nav .nav-lvl2 {
		padding-bottom: 60px;
		min-height: calc(var(--vh-mob, 1vh) * 100)
	}
}

@media screen and (min-width:1191px) {
	.main-nav .nav-lvl0-tab .sub-nav {
		display: block;
		opacity: 0;
		height: 0;
		transition: all .4s ease-out;
		transition-delay: .2s
	}
	.main-nav .nav-lvl0-tab .nav-bg {
		background-image: linear-gradient(to bottom, #fff 400px, rgba(41, 41, 41, 0) 400px);
		transition: height .5s ease-out
	}
	.main-nav .nav-lvl0-tab .nav-bg.sale-menu {
		background-image: linear-gradient(to bottom, #fff 40px, rgba(41, 41, 41, 0) 40px)
	}
	.main-nav .nav-lvl0-tab:hover .sub-nav {
		opacity: 1;
		height: auto;
		pointer-events: auto
	}
	.main-nav .nav-lvl0-tab:hover .nav-bg {
		height: 100%
	}
	.sale-menu .nav-lvl1.m-padding-menu {
		max-width: 400px;
		display: flex;
		justify-content: center;
		align-items: center;
		margin: 0 auto
	}
	.sale-menu .nav-lvl1.m-padding-menu li {
		width: auto
	}
}

