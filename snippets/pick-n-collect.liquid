<div id="pick-n-collect">
  <div class="omni-container">
    <main class="omni-main-content">
      <div class="omni-wrapper">
        <div class="omni-checkoutMethodsContainer">
          <div class="omni-checkoutMethod" id="open-ld-modal">
            <div class="omni-checkoutMethodImage">
              <div class="omni-shippingIcon"><i class="fas fa-shipping-fast fa-3x"></i></div>
            </div>
            <div class="omni-checkoutMethodName">Local Delivery</div>
          </div>
          <div class="omni-checkoutMethod omni-mb-1" id="open-sp-modal">
            <div class="omni-checkoutMethodImage">
              <div class="omni-shippingIcon"><i class="fas fa-store fa-3x"></i></div>
            </div>
            <div class="omni-checkoutMethodName">Store Pickup</div>
          </div>
         <div class="omni-checkoutMethod active">
            <div class="omni-checkoutMethodImage">
             <div class="omni-shippingIcon"><i class="fas fa-globe-americas fa-3x"></i></div> 
            </div>
            <div class="omni-checkoutMethodName">Shipping</div>
          </div>
        </div>
      </div>
    </main>
  </div>
  <!-- The Modal -->
  <div id="myModal" class="omni-modal">
    <!-- Modal content -->
    <div class="omni-modal-content">
      <span class="omni-modal-close"><i class="fas fa-times ico-close-popup"></i></span>
      <div id="omni-checkout-form" action="#">
        <div id="omni-checkout-wizard" class="omni-checkout-wizard">          
          <h3><i class="fas fa-map-marker-alt fa-lg omni-mr-1"></i> <span class="omni-tab-title">Information</span> </h3>
          <section> 
            <div class="omni-payment-wrapper omni-mt-1">
              <div class="omni-payment-area">
                <div class="omni-top-error-area"><div class="omni-error-area"></div></div>
                <div class="omni-customer-login">
                    {% if customer %}
                      {{ customer.name }}<span> | </span>{{ 'layout.customer.log_out' | t | customer_logout_link }}
                    {% else %}
                      Already have an account? {{ 'layout.customer.log_in' | t | customer_login_link }}
                    {% endif %}                                    
                </div>
                <div class="omni-input-fields">
                  <div class="omni-field ">
                    <input type="hidden" id="fulfilment-type" name="attributes[Fulfilment Type]" value="Shipping">
                    <input type="hidden" id="store-name" name="attributes[Store Name]" value="">
                    <input type="hidden" id="store-address" name="attributes[Store Address]" value="">
                    <input type="hidden" id="store-details" name="store-details" value="">
<!--                     <div class="omni-input-wrapper">
                      <label class="omni-form-wizard-label" for="confirm">Find your nearest pickup location *</label>
                      <div class="omni-form-location-map"></div>
                    </div> -->
                  </div>
                </div>
                <div class="omni-field">
                  <div class="omni-input-wrapper">
                    <label class="omni-form-wizard-label " for="confirm">Choose a location *</label>
                    <select id="pickup-location" name="attributes[Selected Store]" style="width: 100%" class="required omni-form-wizard-input" onchange="managePickupDateAndAddress(this);"></select>
                    <span id="pickup-location-error" style="display: none;" class="omni-span-error" for="pickup-location"></span>
                  </div>                          
                  <div class="omni-input-wrapper hide">
                    <label class="omni-form-wizard-label" for="confirm">Choose a date and time *</label>
                    <input id="datetimepicker" name="datetimepicker" type="text" class="required omni-form-wizard-input">
                    <span id="datetimepicker-error" style="display: none;" class="omni-span-error" for="datetimepicker"></span>
                  </div>
                </div>
              </div>
              <div class="omni-right-panel">
                {% if cart.item_count > 0 %}
                  <div class="omni-sidebar-cart-content ">
                    <div class="omni-sidebar-cart-section">
                      <table class="omni-sidebar-cart-product-table">
                        <tbody>
                          {% for item in cart.items %}
                            <tr>
                              <td class="omni-cart-product-image">
                                <div class="">
                                  <div class="omni-cart-product-image-wrapper">
                                    <img class="cart__image" src="{{ item | img_url: '40x40', scale: 2 }}" alt="{{ item.title | escape }}" data-item-url="{{ item.url }}">                                    
                                  </div>                                  
                                </div>
                              </td>                              
                              <td class="omni-cart-product-description">
                                <span class="omni-cart-product-desc-name">
                                  {{ item.product.title }}                    
                                  {% if item.quantity > 1 %}
                              <span class="medium-up--hide"><span class="visually-hidden">{{ 'cart.label.quantity' | t }}</span>(x{{ item.quantity }})</span>
                            {% endif %}
                </span>
                                <span class="omni-cart-product-desc-variant">
                                  {% unless item.variant.title contains 'Default' %}
                                    <div class="cart__meta-text">
                                      {% for option in item.product.options %}
                                        {{ option }}: {{ item.variant.options[forloop.index0] }}<br/>
                                      {% endfor %}
                                    </div>
                                  {% endunless %}                                  
                                </span>
                              </td>
                              <td>X <span class="product-thumbnail__quantity" aria-hidden="true">{{item.quantity}}</span></td>
                              <td class="omni-cart-product-price"><span class="Bold-theme-hook-DO-NOT-DELETE bold_cart_item_total" data-item-key="{{item.key}}" style="display:none !important;"></span>{{ item.final_line_price | money }}</td>
                            </tr>
                          {% endfor %}
                        </tbody>
                      </table>
                      <div class="omni-cart-summary-totals">
                        <table class="omni-cart-total-line-table">
                          <tbody>
                            <tr class="omni-cart-total-line-subtotal">
                              <th class="omni-cart-total-line-name" scope="row">Subtotal</th>
                              <td class="omni-cart-total-line-price">
                                <div class="cart-item__original-price">{{ cart.items_subtotal_price | money }}</div>
                              </td>
                            </tr>
                            <tr class="omni-cart-total-line-subtotal">
                              <th class="omni-cart-total-line-name" scope="row">Discounts</th>
                              <td class="omni-cart-total-line-price">
                                <div class="cart-item__discount">{{ cart.total_discount | money }}</div>
                              </td>
                            </tr>
                            {% if shop.taxes_included == false and customer and customer.tax_exempt == false %}
                              <tr class="omni-cart-total-line-subtotal">
                                <th class="omni-cart-total-line-name" scope="row">Taxes</th>                              
                                <td class="omni-cart-total-line-price">
                  {{ 0.0 | money }}
                                </td>
                              </tr>
                            {% endif %}
                          </tbody>
                          <tfoot class="omni-cart-table-footer">
                            <tr>
                              <th class="omni-cart-table-footer-label" scope="row">Total</th>
                              <td class="omni-cart-table-footer-amount">{{ cart.original_total_price | money }}</td>
                            </tr>
                          </tfoot>
                        </table>
                      </div>
                    </div>
                  </div>
                {% endif %}
              </div>
            </div>
          </section>
          <h3><i class="fas fa-address-book fa-lg omni-mr-1"></i><span class="omni-tab-title">Contact</span></h3>
          <section>
            <div class="omni-payment-wrapper omni-mt-1">
