<script type="text/javascript">
  
  // This function is to load paypal SDK
  function loadPaypalSDK()
  {
    var paypalClientId = '';
    var paypalSecret = '';
    var currency = {{cart.currency.iso_code | json}};
    var currency = "PHP";
    var omnirioSetting = sessionStorage.getItem('omnirio');
    if(omnirioSetting != undefined && omnirioSetting.length > 0)
    { 
      var omnirioSetting = JSON.parse(omnirioSetting);
      var paymentMethods = omnirioSetting.payment_methods;
      $.each(paymentMethods, function(key,value) {
        if (value.code == 'paypal')
        {
          paypalClientId = value.client_id;
          paypalSecret = value.secret;
          return false;
        }
      });
    }
    if (paypalClientId.length > 0 && paypalSecret.length > 0)
    {
      var script = document.createElement('script');
      script.type = 'text/javascript';
      script.src = "https://www.paypal.com/sdk/js?client-id="+paypalClientId+"&currency="+currency;
      document.getElementsByTagName('head')[0].appendChild(script);
    }  
  }
  
  // This function displays Smart Payment Buttons on your web page.  
  function loadPaypalMethod()
  {
    paypal.Buttons({
      createOrder: function(data, actions) {
        // This function sets up the details of the transaction, including the amount and line item details.
        return actions.order.create({
          purchase_units: [{
            amount: {
              value: {{cart.total_price | divided_by: 100.0 | json}}
            },
            currency: {
              value: {{cart.currency.iso_code | json}}
            }  
          }]
        });
      },
      onApprove: function(data, actions) {
        // This function captures the funds from the transaction.
        return actions.order.capture().then(function(details) {
          // This function shows a transaction success message to your buyer.
          $('#omni-payment-method').val(JSON.stringify(details));
          var message = ('Thanks ' + details.payer.name.given_name+ '! for the successful payment.');                 
          addFlashErrors([message]);
          $('.actions > ul > li:first-child').attr('style', 'display:none');
          $('.omni-modal-close').attr('style', 'display:none');
          var orderFlag = createShopifyOrder(this);
        });
      }
    }).render('#paypal-button-container');
  }
  
  // Refund paypal call
  function paypalRefundCall()
  {
    var url = '';
    var method_type = '';
    var message = '';
    var paypalClientId = '';
    var paypalSecret = '';
    var acccessToken = '';
    var omnirioSetting = sessionStorage.getItem('omnirio');
    if(omnirioSetting != undefined && omnirioSetting.length > 0)
    { 
      var omnirioSetting = JSON.parse(omnirioSetting);
      var paymentMethods = omnirioSetting.payment_methods;
      $.each(paymentMethods, function(key,value) {
        if (value.code == 'paypal')
        {
          paypalClientId = value.client_id;
          paypalSecret = value.secret;
          return false;
        }
      });
      
      var paidDetails = JSON.parse($('#omni-payment-method').val());
      $.each(paidDetails.purchase_units[0].payments.captures[0].links, function(key,value) {
        if (value.rel == 'refund')
        {
          url = value.href;
          method_type = value.method;
          return false;
        } 
      });

      //var auth_url = "https://api.sandbox.paypal.com/v1/oauth2/token";
      var auth_url = "https://api.paypal.com/v1/oauth2/token";
      basicAuth = window.btoa(paypalClientId+":"+paypalSecret);
      var settings = {
        "url": auth_url,
        "method": method_type.toUpperCase(),
        "timeout": 0,
        "headers": {
          "Authorization": ("Basic "+ basicAuth),
          "Content-Type": "application/x-www-form-urlencoded"
        },
        "data": {
          "grant_type": "client_credentials"
        }
      };

      $.ajax(settings).done(function (response) {
        acccessToken = response.access_token;
        $.ajax({
          url: url,
          type: method_type.toUpperCase(),
          contentType: 'application/json',
          headers: {'Authorization': ('Bearer '+acccessToken), 'Content-Type': 'application/json'},
               data : {}     
               }).done(function() {
          addFlashErrors(['Your refund has been processed, you will receive your payment within next 2 to 3 working days!']);
        });              
      });
    }
    return message;  
  }
                           
