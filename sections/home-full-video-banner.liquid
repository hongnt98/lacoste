<div  class="js-mcontent-strate mcontent-strate mcontent-cover l-relative l-vmargin-row-and-half " id="ModularContent_Strate_Cover_WindowFW19_Xmas" tabindex="-1" data-strate-title="Cadeaux De NoÃ«l">
    <div class="l-overflow-hidden ">
        <video playsinline="true" autoplay="true" muted="true" loop="true" class="img-cover img-cover--top" width="101%" height="auto">
            <source data-desktop="{{ section.settings.desktop_video }}" data-mobile="{{ section.settings.mobile_video }}" type="video/mp4">
        </video>
    </div>
    <div class="section-title l-overlay pointer-none  flex flex--col above cover-padding flex--justify-end  flex-m--align-start text-m-initial flex-m--justify-end pointer-none padding-x1">
